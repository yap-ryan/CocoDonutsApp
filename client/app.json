{
  "expo": {
    "name": "CocoDonutsApp",
    "slug": "CocoDonutsApp",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/cocoicon1024.png",
    "splash": {
      "image": "./assets/cocosplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*",
      "assets/donuts/*",
      "assets/fonts/*"

    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Coco Donuts app for rewards "
  }
}
